<section class="pb-medium content" data-scroll-section>
  <div class="container">
    <div class="row">
      <div class="col-lg-6">
        {{ content | safe}}
      </div>
      <div class="col-lg-6 sticky">
        <div class="sticky overflow-hidden border">
          <iframe src="https://www.google.com/maps/{{ maplink }}" width="100%" style="border:0; height:68vh;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
      </div>
    </div>
  </div>
</section>

<section class="pb-medium" data-scroll-section>
  <div class="container">
    <div class="line-x lx-op"></div>
    <div class="towns-links-wrap pt-small">
        <ul>
          {% for post in collections.towns %}
          <li>
            <a href="{{ post.url }}" title="{{ post.data.title }}" class="link-hover">
              {{ post.data.title }}
            </a>
          </li>
          {% endfor %}
        </ul>
    </div>
  </div>
</section>     



